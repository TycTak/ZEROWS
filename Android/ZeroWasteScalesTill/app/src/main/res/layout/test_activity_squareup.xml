<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Test_Activity_SquareUp">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <Button
            android:text="TEST NO DATA RETURNED BUT OK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="btnNoDataReturned"/>

        <Button
            android:text="TEST NO DATA RETURNED BUT NOT OK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="btnNoDataReturnedNotOk"/>

        <Button
            android:text="TEST DATA RETURNED AND NOT OK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="btnDataReturnedNotOk"/>

        <Button
            android:text="TEST DATA RETURNED AND OK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="btnDataReturned"/>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>